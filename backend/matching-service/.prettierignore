# Ignore artifacts:
build
dist
coverage
node_modules
*.min.js
*.bundle.js

# Ignore all HTML files:
*.html

# Ignore package files (auto-generated)
package-lock.json
yarn.lock

# Ignore environment files
.env
.env.*
!.env.example
!.env.sample

# Ignore Docker files
Dockerfile
docker-compose.yml
.dockerignore

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
logs/

# Ignore README files (to preserve manual formatting)
*.md

# Ignore IDE files
.vscode/
.idea/
*.swp
*.swo

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore Git
.git/
.gitignore
